{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "ls -la /app/types/SocketSchemas && cd /app/client && npm install && npm run build"
  },
  "deploy": {
    "startCommand": "cd /app/client && vite preview",
    "outputDirectory": "dist/client",
    "sleepApplication": false
  }
}